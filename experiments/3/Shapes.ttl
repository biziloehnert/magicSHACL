@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix : <http://example.org/> .

:ActorShape a sh:NodeShape;
	sh:property [ 
		sh:path dbo:birthPlace;
		sh:node :LocationShape
	] ;
	sh:property [
		sh:path dbo:activeYearsStartYear;
		sh:minCount 1
	] ;
	sh:property [
		sh:path dbo:occupation;
		sh:hasValue dbr:Actor ;
		sh:minCount 1
	];
	sh:property [
		sh:path dbo:imdbId;
		sh:minCount 1
	] ;
	sh:property [
	sh:path[
		sh:inversePath dbo:starring;
	];
	sh:minCount 1
	] .

:DirectorShape a sh:NodeShape;
	sh:property [
		sh:path dbo:birthPlace;
		sh:node :LocationShape
	] ;
	sh:property [
		sh:path dbo:imdbId;
		sh:minCount 1
	] ;
	sh:property [
		sh:path[
			sh:inversePath dbo:director;
		];
		sh:minCount 1
	] .
   
:LocationShape a sh:NodeShape;
	sh:property [
		sh:path dbo:country;
		sh:minCount 1
	] ;
	sh:property [
		sh:path dbo:leaderTitle;
		sh:minCount 1
	] ;
	sh:property [
		sh:path dbo:populationDensity;
		sh:minCount 1
	] .

:AnarchyShape a sh:NodeShape;
	sh:property [
		sh:path dbo:leaderTitle ;
		sh:maxCount 0;
	] .

:MovieShape a sh:NodeShape;
	sh:property [
		sh:path dbo:writer;
		sh:minCount 1
	] ;
	sh:property [
		sh:path dbo:musicComposer;
		sh:minCount 1
	] ;
	sh:property [
		sh:path dbo:director;
		sh:node :DirectorShape;
		sh:minCount 1
	] ;
	sh:property [
		sh:path dbo:imdbId;
		sh:minCount 1;
		sh:maxCount 1
	];
	sh:property [
		sh:path dbo:starring;
		sh:node :ActorShape;
		sh:minCount 1
	] .

:TranslatedMovieShape a sh:NodeShape;
	sh:node :MovieShape;
	sh:property [
		sh:path dbo:language;
		sh:minCount 2
	] ;
.

:InfluencerShape a sh:NodeShape;
	sh:property [ 
		sh:path dbo:influenced;
		sh:minCount 1;
	] ;	
.

:AwardWinningShape a sh:NodeShape;
	sh:property [ 
		sh:path dbo:award;
		sh:minCount 1;
	] ;		
.

:FamousShape a sh:NodeShape;
	sh:property [ 
		sh:path dbo:knownFor;
		sh:minCount 1;
	] ;		
.

:PersonShape a sh:NodeShape;
	sh:targetClass dbo:Actor;
	sh:property [ 
		sh:path dbo:birthPlace;
		sh:minCount 1;
		sh:maxCount 1;
		sh:node :LocationShape
	] ;
	sh:property [ 
		sh:path dbo:birthDate;
		sh:minCount 1;
		sh:maxCount 1;
	] ;
	sh:property [ 
		sh:path dbo:birthName;
		sh:minCount 1;
	] ; 
.

:MusicianShape a sh:NodeShape;
	sh:node :PersonShape;
	sh:property [ 
		sh:path dbo:instrument;
		sh:minCount 1;
	] ;
.

:ParentShape a sh:NodeShape;
	sh:node :PersonShape;
	sh:property [ 
		sh:path dbo:child;
		sh:minCount 1;
	] ;
.

:ReligiousPersonShape a sh:NodeShape;
	sh:node :PersonShape;
	sh:property [ 
		sh:path dbo:religion;
		sh:minCount 1;
	] ;
.

:DeadPersonShape a sh:NodeShape;
	sh:node :PersonShape;
	sh:property [ 
		sh:path dbo:deathDate;
		sh:minCount 1;
	] ;
	sh:property [ 
		sh:path dbo:deathPlace;
		sh:minCount 1;
	] ;
.

:LivingLanguage a sh:NodeShape;
	sh:property [
		sh:path dbo:spokenIn;
		sh:minCount 1;
	];
.