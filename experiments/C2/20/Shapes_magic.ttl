@prefix dbo: <http://dbpedia.org/ontology/>.
@prefix dbr: <http://dbpedia.org/resource/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix : <http://example.org/>.
        		
:WorkingClassShape_magic
	a sh:NodeShape ;
	sh:targetClass dbo:Person ;.

	:PersonShape_magic
	a sh:NodeShape ;
	sh:node :WorkingClassShape_magic ;
	sh:node :WorkingPersonShape_magic ;
	sh:node :ParentShape_magic ;
	sh:path dbo:child ;
	sh:inversePath dbo:child ;.

	:WorkingParentShape_magic
	a sh:NodeShape ;
	sh:node :WorkingClassShape_magic ;
	sh:path dbo:child ;.

	:ParentShape_magic
	a sh:NodeShape ;
	sh:node :WorkingParentShape_magic ;.

	:WorkingPersonShape_magic
	a sh:NodeShape ;
	sh:node :WorkingParentShape_magic ;.

	:EmployeeShape_magic
	a sh:NodeShape ;
	sh:node :WorkingPersonShape_magic ;
	sh:node :NotEmployeeShape_magic ;.

	:NotEmployeeShape_magic
	a sh:NodeShape ;
	sh:node :EmployeeShape_magic ;
	sh:inversePath dbo:employer ;.

	:LocationShape_magic
	a sh:NodeShape ;
	sh:node :PersonShape_magic ;
	sh:inversePath dbo:birthPlace ;
	.	
				
:WorkingClassShape
	sh:node :PersonShape ;
	sh:node :WorkingClassShape_magic ;
	sh:property [
		sh:path [
		sh:inversePath dbo:child ;
	];
		sh:minCount 1 ;
		sh:node :WorkingParentShape ;
	];
	a sh:NodeShape ;
.

:WorkingParentShape
	sh:node :ParentShape ;
	sh:node :WorkingPersonShape ;
	sh:node :WorkingParentShape_magic ;
	a sh:NodeShape ;
.

:WorkingPersonShape
	sh:node :PersonShape ;
	sh:node :EmployeeShape ;
	sh:node :WorkingPersonShape_magic ;
	a sh:NodeShape ;
.

:EmployeeShape
	sh:node :EmployeeShape_magic ;
	sh:property [
		sh:path dbo:employer ;
		sh:minCount 1 ;
		sh:node :NotEmployeeShape ;
	];
	a sh:NodeShape ;
.

:NotEmployeeShape
	sh:not [
		sh:node :EmployeeShape ;
	];
	sh:node :NotEmployeeShape_magic ;
	a sh:NodeShape ;
.

:PersonShape
	sh:node :PersonShape_magic ;
	sh:property [
		sh:path dbo:birthPlace ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:node :LocationShape ;
	];
	a sh:NodeShape ;
.

:LocationShape
	sh:node :LocationShape_magic ;
	sh:property [
		sh:path dbo:country ;
		sh:minCount 1 ;
	];
	a sh:NodeShape ;
.

:ParentShape
	sh:node :PersonShape ;
	sh:node :ParentShape_magic ;
	sh:property [
		sh:path dbo:child ;
		sh:minCount 1 ;
	];
	a sh:NodeShape ;

.